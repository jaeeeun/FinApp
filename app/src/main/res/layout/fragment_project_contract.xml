<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProjectContractFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="8.2sp"
        android:textColor="#99000000"
        android:textSize="15.8sp"
        tools:text="영상물 지급 계약" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:letterSpacing="0.02"
        android:lineSpacingExtra="6.2sp"
        android:textColor="#de000000"
        android:textSize="13.8sp"
        tools:text="          와             은 계약건명에 명시된 업무작업을 수행하기 위해 다음과 같이 계약을 체결한다."
/>
    </LinearLayout>
    <!-- TODO: Update blank fragment layout -->



    <Button
        android:id="@+id/project_contract_button"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:layout_gravity="bottom"
        android:background="@color/blue_violet"
        android:text="프로젝트 생성"
        android:textColor="@color/white_three"
        android:textSize="19.7sp" />

</FrameLayout>